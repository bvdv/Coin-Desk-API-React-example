  "homepage": "https://github.com/bvdv/Coin-Desk-API-React-example#readme",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/runtime": "^7.13.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
